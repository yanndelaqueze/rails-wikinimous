<div class="container">
  <%= form_with model: @article do |f| %>
    <%= f.label :title , class: "form-label mt-3" %>
    <%= f.text_field :title , class: "form-control mt-3" %>
    <%= f.label :content , class: "form-label mt-3" %>
    <%= f.text_field :content , class: "form-control mt-3" %>
    <%= f.submit class: "btn btn-warning mt-3"%>
  <% end %>
</div>
